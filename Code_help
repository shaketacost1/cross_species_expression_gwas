shaketaveal@Shaketas-MacBook-Pro human % TARGET=CTCF
ASSEMBLY=GRCh38
shaketaveal@Shaketas-MacBook-Pro human % curl -sG "https://www.encodeproject.org/search/" \
  --data-urlencode "type=File" \
  --data-urlencode "file_format=bed+narrowPeak" \
  --data-urlencode "output_type=peaks" \
  --data-urlencode "assembly=${ASSEMBLY}" \
  --data-urlencode "target.label=${TARGET}" \
  --data-urlencode "status=released" \
  --data-urlencode "format=json" \
  --data-urlencode "limit=all" \
  -H "Accept: application/json" \
| tee enc_${TARGET}_raw.json \
| jq -r '.["@graph"][].href' \
| sed 's|^|https://www.encodeproject.org|g' > enc_${TARGET}_urls.txt
shaketaveal@Shaketas-MacBook-Pro human % jq '.["@graph"] | length' enc_${TARGET}_raw.json
wc -l enc_${TARGET}_urls.txt
0
       0 enc_CTCF_urls.txt
shaketaveal@Shaketas-MacBook-Pro human % head enc_${TARGET}_urls.txt
shaketaveal@Shaketas-MacBook-Pro human % 
